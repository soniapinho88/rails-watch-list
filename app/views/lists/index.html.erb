<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <div class="container-fluid">
    <a class="navbar-brand" href="#">
      <img src="data:image/png;base64," />
    </a>
    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav me-auto">
          <li class="nav-item active">
            <a class="nav-link"> <%= link_to "My lists", lists_path %> </a>
          </li>
          <li class="nav-item">
            <a class="nav-link"> <%= link_to "New list", new_list_path %> </a>
          </li>
      </ul>
    </div>
  </div>
</div>

<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://www.vmcdn.ca/f/files/burnabynow/images/burnabynow/abhinaya-natesh-folder/gettyimages-1150049038.jpg;w=960);">
  <div class="container">
    <h1>Save any kind of movies!</h1>
  </div>
</div>

<div class="movie-category">
  <p class="category-title">MY LISTS</p>
  <%= link_to "Create a new list", new_list_path, class: "btn btn-dark" %>
</div>

<div class="cards">
  <% @lists.each do |list| %>
    <div class="card-category">
      <h3 class="list-title"><%= link_to list.name, list_path(list)%></h3>
    </div>
  <% end %>
</div>
